profile stationary_thunderbolt{
	output DP-5 enable 
	output eDP-1 disable 
}
profile stationary_hdmi{
	output eDP-1 disable
	output DP-3 mode 1920x1080 position 5778,0
}
profile stationary_typeC{
	output eDP-1 disable
	output DP-1 mode 1920x1080 position 5778,0
}

profile nomad{
	output eDP-1 enable scale 2
}
